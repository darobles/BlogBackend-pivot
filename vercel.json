{
  "framework": null,
  "builds": [
    {
      "src": "core/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "30mb", 
        "runtime": "python3.12"
      }
    }
  ],
  "functions": {
    "core/wsgi.py": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "core/wsgi.py",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS, DELETE, PUT"
      }
    },
    {
      "src": "/(.*)",
      "dest": "core/wsgi.py"
    }
  ]
}